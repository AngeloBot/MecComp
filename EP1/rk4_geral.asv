function [ Yout ] = rk4_geral( F , t_i, t_f, h, Y0 )
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here

Yout = Y0;
Y = Y0;

for t=t_i:h:t_f
    for j=1:1:length(F)
        k1 = F(t, Y);
        k2 = F(t, Y.*k1);
        k3 = F(t, Y.*k2);
    end
    Y = Y + (h/6)*(k1 + 2*k2 + 2*k3 + k4);
    Yout = [Yout, Y];
end

end

