function [ Y, ddth, t ] = rk2(F , t_i, t_f, h, Y0)
%UNTITLED3 Summary of this function goes here
%   Detailed explanation goes here

Yout = Y0;
Y = Y0;
dy0dt = F(t_i,Y0);
dydt = dy0dt;

for t = (t_i+h):h:t_f
    % Cálculo dos termos k's
    k1 = F(t, Y);
    k2 = F(t + h/2, Y + (k1*h/2));
    
    % Cálculo do y(i+1)
    Y = Y + (h/6)*(k1 + 2*k2 + 2*k3 + k4);
    Yout = [Yout, Y];
    dydt = [dydt, k1];
end
end